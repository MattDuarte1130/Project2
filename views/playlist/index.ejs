<%-include('../partials/header.ejs') %>
<%- include('../partials/nav.ejs') %>
<div class="container">
<h1>Playlist Index Page</h1>
 <ul>
     <% for(let i = 0; i < playlist.length; i++){ %>
       <div class="itemContainer">
       <li>
         <h3 class='playlistTitle'><%=playlist[i].title; %></h3>
           <a href="/playlist/<%=playlist[i].id; %>"><img src="<%=playlist[i].img; %>" alt="" width="320" height="180"></a><br>
         </li>
         </div>
     <% } %>

</ul>

<%-include('../partials/footer.ejs') %>
</div>
